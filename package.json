{
  "name": "react-msa-viewer",
  "description": "A React component to create Multiple Sequence Alignments",
  "keywords": [
    "react",
    "component",
    "msa",
    "alignment",
    "viewer",
    "multiple",
    "sequence",
    "plotly"
  ],
  "homepage": "https://github.com/plotly/react-msa-viewer",
  "repository": "https://github.com/plotly/react-msa-viewer",
  "bugs": "https://github.com/plotly/react-msa-viewer/issues",
  "author": "Plotly, Inc",
  "license": "MIT",
  "version": "1.0.1",
  "private": false,
  "main": "dist/index.umd.min.js",
  "module": "dist/index.esm.min.js",
  "scripts": {
    "build:basic": "NODE_ENV=production rollup -c",
    "build:lib-esm": "NODE_ENV=production rollup -c --format es --file dist/index.esm.js && ./terser.js dist/index.esm.js dist/index.esm.min.js",
    "build:lib-umd": "NODE_ENV=production rollup -c && ./terser.js dist/index.umd.js dist/index.umd.min.js",
    "build:lib": "npm run build:lib-umd && npm run build:lib-esm",
    "netlify": "npm run build-storybook && npm run build:lib && mv dist storybook-static",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "release-it": "npm run build:lib && npm run docs",
    "docs": "./docs/build",
    "watch": "rollup -w -c",
    "serve": "http-server -p8887 dist"
  },
  "peerDependencies": {
    "react": ">= 15 < 17",
    "react-dom": ">= 15 < 17"
  },
  "sideEffects": false,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-replace": "2.3.1",
    "@rollup/plugin-strip": "1.3.2",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-options": "5.3.18",
    "@storybook/addon-storysource": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/react": "5.3.18",
    "babel-jest": "25.2.6",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "http-server": "^0.12.1",
    "immutable": "4.0.0-rc.12",
    "jest": "25.2.6",
    "jest-canvas-mock": "2.2.0",
    "react": "16.13.1",
    "react-docgen": "5.3.0",
    "react-docgen-external-proptypes-handler": "2.0.0",
    "react-dom": "16.13.1",
    "rollup": "2.3.2",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-visualizer": "4.0.2"
  },
  "dependencies": {
    "lodash-es": "4.17.15",
    "plotly-icons": "1.3.14",
    "prop-types": "15.7.2",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "reselect": "4.0.0"
  }
}
